{
    "version": 2,
    "functions": {
      "api/ssl-check.js": {
        "runtime": "@vercel/node"
      }
    },
    "rewrites": [
      {
        "source": "/ssl-check",
        "destination": "/api/ssl-check"
      }
    ],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, OPTIONS"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type"
          }
        ]
      }
    ]
  }